<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
    <servlet-name>UserLoginServlet</servlet-name>
    <servlet-class>servlet.UserLoginServlet</servlet-class>
  </servlet>
  <servlet>
  <servlet-name>UserRegisterServlet</servlet-name>
  <servlet-class>servlet.UserRegisterServlet</servlet-class>
</servlet>
    <servlet>
        <servlet-name>GetScore1</servlet-name>
        <servlet-class>servlet.GetScore1</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>GetScore2</servlet-name>
        <servlet-class>servlet.GetScore2</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>GetScore3</servlet-name>
        <servlet-class>servlet.GetScore3</servlet-class>
    </servlet>
    <servlet-mapping>
    <servlet-name>UserLoginServlet</servlet-name>
    <url-pattern>/UserLoginServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>UserRegisterServlet</servlet-name>
    <url-pattern>/UserRegisterServlet</url-pattern>
  </servlet-mapping>
    <servlet-mapping>
        <servlet-name>GetScore1</servlet-name>
        <url-pattern>/GetScore1</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>GetScore2</servlet-name>
        <url-pattern>/GetScore2</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>GetScore3</servlet-name>
        <url-pattern>/GetScore3</url-pattern>
    </servlet-mapping>

</web-app>
